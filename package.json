{
  "name": "lifetime-sites",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "prebuild:lifetime": "node scripts/generate-headers.js",
    "build:lifetime": "npx @11ty/eleventy --config=eleventy.lifetime.js",
    "build:aih": "npx @11ty/eleventy --config=eleventy.aih.js",
    "build:cc": "npx @11ty/eleventy --config=eleventy.cc.js",
    "build:all": "npm run build:lifetime && npm run build:aih && npm run build:cc"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "rimraf": "^5.0.5"
  }
}

