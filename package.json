{
  "name": "lifetime-home-services-website",
  "version": "1.0.0",
  "description": "Tri-brand website for Lifetime Home Services, Closet Concepts, and America In-Home",
  "scripts": {
    "build": "npm run build:all",
    "build:all": "npm run build:lifetime && npm run build:cc && npm run build:aih",
    "build:lifetime": "npx @11ty/eleventy --config=eleventy.lifetime.js",
    "build:cc": "npx @11ty/eleventy --config=eleventy.cc.js", 
    "build:aih": "npx @11ty/eleventy --config=eleventy.aih.js",
    "build:safe": "npm run build:all && node scripts/postbuild.cjs",
    "serve": "npx @11ty/eleventy --config=eleventy.lifetime.js --serve",
    "serve:cc": "npx @11ty/eleventy --config=eleventy.cc.js --serve",
    "serve:aih": "npx @11ty/eleventy --config=eleventy.aih.js --serve"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1"
  },
  "devDependencies": {}
}

